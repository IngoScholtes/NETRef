@Article{Haralick:1983:IPR,
  author =       "Robert M. Haralick",
  title =        "An interpretation for probabilistic relaxation",
  journal =      j-CVGIP,
  volume =       "22",
  number =       "3",
  pages =        "388--395",
  month =        jun,
  year =         "1983",
  CODEN =        "CVGPDB",
  ISSN =         "0734-189X",
  bibdate =      "Thu Feb 6 18:44:11 MST 1997",
  bibsource =    "Compendex database;
                 http://www.math.utah.edu/pub/tex/bib/cvgip.bib;
                 Misc/misc.1.bib",
  note =         "HARALICK83a",
  abstract =     "Probabilistic relaxation has been the basis of one of
                 the popular cooperative processing mechanisms used in
                 image analysis. It has been a mechanism whose theory
                 has not been well understood. In this paper, some
                 general conditional independence conditions are stated
                 which give probabilistic relaxation the interpretation
                 that each iteration computes the conditional
                 probability of each local label given a new context
                 which is the context of the previous iteration enlarged
                 by one neighbourhood width. This interpretation implies
                 that relaxation iterations must only continue until the
                 conditional independence assumptions no longer hold or
                 until the entire context is taken into account,
                 whichever comes first.",
  acknowledgement = ack-nhfb,
  classification = "723",
  fjournal =     "Computer Vision, Graphics, and Image Processing",
  journalabr =   "Comput Vision Graphics Image Process",
  keywords =     "image processing",
}

